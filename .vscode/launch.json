{
  // https://code.visualstudio.com/docs/editor/variables-reference
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Active Js File",
      "runtimeExecutable": "node",
      "args": ["${file}"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Active Ts File",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/node_modules/ts-node/dist/bin.js",
      "windows": {
        "program": "${workspaceFolder}/node_modules/ts-node/bin/ts-node"
      },
      "args": ["-O", "{\"module\": \"commonjs\"}", "--files", "${file}"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest Active File",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "windows": {
        "program": "${workspaceFolder}/node_modules/jest/bin/jest"
      },
      "args": ["${fileBasenameNoExtension}", "--config", "jest.config.js"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Release",
      "runtimeExecutable": "/Users/ender/.nvm/versions/node/v16.15.0/bin/node",
      "program": "${workspaceFolder}/node_modules/ts-node/dist/bin.js",
      "windows": {
        "program": "${workspaceFolder}/node_modules/ts-node/bin/ts-node"
      },
      "args": [
        "-O",
        "{\"module\": \"commonjs\"}",
        "--files",
        "${workspaceFolder}/packages/release/bin/release.js",
        // "-h",
        "--no-git-checks",
        // "--tag",
        // "next",
        // "minor"
        "--branch",
        "develop"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Gen",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/node_modules/ts-node/dist/bin.js",
      "windows": {
        "program": "${workspaceFolder}/node_modules/ts-node/bin/ts-node"
      },
      "args": [
        "-O",
        "{\"module\": \"commonjs\"}",
        "--files",
        "${workspaceFolder}/packages/create/bin/gen.js"
      ],
      "cwd": "/Users/ender/learning/github/templates/packages/npm-node",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Create Template",
      "runtimeExecutable": "node",
      // "program": "${workspaceFolder}/node_modules/ts-node/dist/bin.js",
      "args": [
        "${workspaceFolder}/packages/create-template/bin/create-template",
        "temp",
        "-f"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
